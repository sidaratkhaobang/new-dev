{
    "name": "lubianfuchen/dusk-dashboard",
    "description": "A beautiful dashboard for your Laravel Dusk tests",
    "keywords": [
        "lubianfuchen",
        "dusk-dashboard"
    ],
    "homepage": "https://github.com/lubianfuchen/dusk-dashboard",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de",
            "homepage": "https://Lubianfuchen.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "cboden/ratchet": "^0.4.1",
        "clue/buzz-react": "^2.5",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/console": "5.6.*|5.7.*|5.8.*|6.*|7.*|8.*|^9.0",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*|7.*|8.*|^9.0",
        "laravel/dusk": "^4.0|^5.0|^6.0|^7",
        "yosymfony/resource-watcher": "^3.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Lubianfuchen\\DuskDashboard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lubianfuchen\\DuskDashboard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lubianfuchen\\DuskDashboard\\DuskDashboardServiceProvider"
            ]
        }
    }
}
